<!DOCTYPE html>
<html ng-app="mockup">
    <head>
        <meta charset="UTF-8">

        <title>FlipG</title>
        
        <link rel='stylesheet prefetch' href='http://fonts.googleapis.com/css?family=Roboto:400,700|Roboto+Condensed:100,400|Roboto+Slab:100,400' />
        <link rel='stylesheet prefetch' href='css/font-awesome.min.css' />
        <link rel="stylesheet" href="css/style.css" type="text/css" />
    </head>

    <body ng-controller="DashboardController as dashboard">

        <nav>
            <ul>
                <li><img id="logoFlipG" src="img/logo.png" alt="FlipG" /></li>
                <li>
                    <form>
                        <input type="text" class="search" placeholder="Search for any blog..." />
                    </form>
                </li>
            </ul>
        </nav>
        <div class="wrapper cf">
            <ul class="grid">
                <li class="class1" ng-repeat="feed in dashboard.feeds">
                    <div>
                        <h3>{{feed.title}}</h3>
                        <p>{{feed.text}}</p>
                    </div>
                </li>
            </ul>  
        </div>

        <script src="js/index.js"></script>
        <script type="text/javascript" src="js/angular.js"></script>
        <script type="text/javascript" src="js/app.js"></script>
        <script type="text/javascript" src="js/jquery-2.1.3.js"></script>
    </body>

</html>
